<% include ../layouts/doctype.ejs %>




<% for(let i=0; i < visuels.length; i++) {%>



<p><%= visuels[i].id %></p>
<p><%= visuels[i].nom_visuel %></p>
<img src="/uploads/<%= visuels[i].image %>" alt="/uploads/<%= visuels[i].image %>"/>





<a class="btn btn-sm btn-info" href="/visuels/detail/<%= visuels[i].id %>" type="button"><i class="fas fa-eye"></i></a>

<a class="btn btn-sm btn-warning" href="/visuels/edit/<%= visuels[i].id %>"><i class="fa fa-pencil-alt"></i></a>

<a class="btn btn-sm btn-danger" href="/visuels/delete/<%= visuels[i].id %>" type="button"><i
class="fas fa-times-circle"></i></a>


<% } %>




<% include ../layouts/doctype_end.ejs %>